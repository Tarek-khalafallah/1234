// Site top header preferences
var coll = document.getElementsByClassName("btn-collapse");
var i;
for (i = 0; i < coll.length; i++) {
	coll[i].addEventListener("click", function() {
		this.classList.toggle("activecoll");
		var collcont = this.nextElementSibling;
		if (collcont.style.maxHeight){
			collcont.style.maxHeight = null;
		}
		else {
			collcont.style.maxHeight = collcont.scrollHeight + "px";
		}
	});
}

