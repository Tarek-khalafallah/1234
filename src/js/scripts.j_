function lightrtpbox_open(lightRTPBoxVideo) {
  //var lightBoxVideo = document.getElementById("VideoId");
  window.scrollTo(0, 0);
  document.getElementById('rtplight').style.display = 'block';
  document.getElementById('rtpfade').style.display = 'block';
  lightRTPBoxVideo.play();
}

function lightrtpbox_close() {
  var lightRTPBoxVideo = document.getElementById("rtpVideoId");
  document.getElementById('rtplight').style.display = 'none';
  document.getElementById('rtpfade').style.display = 'none';
  lightRTPBoxVideo.pause();
}

