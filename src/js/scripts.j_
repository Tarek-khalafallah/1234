$(document).ready(function () {
  $("#checkallstack").change(function () {
    var checked = $(this).is(':checked'); // Checkbox state
    // Select all
    if (checked) {
      $('.check-iStack').each(function () {
        $(this).prop('checked', true);
      });
    } else { // Deselect All
      $('.check-iStack').each(function () {
        $(this).prop('checked', false);
      });
    }
  });
  // Changing state of Checkbox
  $(".stack-container .check-iStack").change(function () {
    // When total options equals to total selected option
    if ($(".stack-container .check-iStack").length == $('[name="stackcbox[]"]:checked').length) {
      $("#checkallstack").prop("checked", true);
    } else {
      $("#checkallstack").prop('checked', false);
    }
  });
});
function clearStack() {
  $('.check-iStack').each(function () {
    $(this).prop('checked', false);
  });
}

