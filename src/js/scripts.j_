// Change maps layout
$(document).ready(function(){
  $("input[name$='changelayout']").click(function(){
  var radio_value = $(this).val();
  if(radio_value=='1') {
    $("#container-content-map-sub").hide();
    $("#container-content-map-main").removeClass("container-content-hor");
    $("#container-content-map-main").removeClass("container-content-ver");
    $("#container-content-map-main").addClass("container-content-full");
    $("#layout-full-icon").addClass("active");
    $("#layout-ver-icon").removeClass("active");
    $("#layout-hor-icon").removeClass("active");
		
  }
  else if(radio_value=='2') {
    $("#container-content-map-sub").show();
    $("#container-content-map-main").removeClass("container-content-hor");
    $("#container-content-map-main").removeClass("container-content-full");
    $("#container-content-map-main").addClass("container-content-ver");
    $("#container-content-map-sub").removeClass("container-content-hor");
    $("#container-content-map-sub").removeClass("container-content-full");
    $("#container-content-map-sub").addClass("container-content-ver");
    $("#layout-full-icon").removeClass("active");
    $("#layout-ver-icon").addClass("active");
    $("#layout-hor-icon").removeClass("active");
  }
  else if(radio_value=='3') {
    $("#container-content-map-sub").show();
    $("#container-content-map-main").removeClass("container-content-full");
    $("#container-content-map-main").removeClass("container-content-ver");
    $("#container-content-map-main").addClass("container-content-hor");
    $("#container-content-map-sub").removeClass("container-content-full");
    $("#container-content-map-sub").removeClass("container-content-ver");
    $("#container-content-map-sub").addClass("container-content-hor");
    $("#layout-full-icon").removeClass("active");
    $("#layout-ver-icon").removeClass("active");
    $("#layout-hor-icon").addClass("active");
   }
  });
  $('[name="changelayout"]:checked').trigger('click');
});

