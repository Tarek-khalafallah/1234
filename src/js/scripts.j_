// Site top header tabs
function openstyle(evt, styleName) {
  var i, x, tablinks;
  x = document.getElementsByClassName("style");
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName("tablink");
  for (i = 0; i < x.length; i++) {
    tablinks[i].className = tablinks[i].className.replace(" module-tab-act", "");
  }
  document.getElementById(styleName).style.display = "block";
  evt.currentTarget.className += " module-tab-act";
}

